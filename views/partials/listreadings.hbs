<table class="ui table">
    <thead>
    <tr>
        <th>Code</th>
        <th>Temp</th>
        <th>Wind Speed</th>
        <th>Pressure</th>
    </tr>
    </thead>
    <tbody>
    {{#each station.readings}}
        <tr>
            <td>
                {{code}}
            </td>
            <td>
                {{temp}}
            </td>
            <td>
                {{wind}}
            </td>
            <td>
                {{pressure}}
            </td>
        </tr>
    {{/each}}
    </tbody>
</table>