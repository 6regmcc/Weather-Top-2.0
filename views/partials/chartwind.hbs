<div class="ui segment" id="chartwind"></div>
<script>
  data = {
    labels: [
      {{#each data.trendData.arrDates}}
        "{{getLocalTime this}}",
      {{/each}}
    ],
    datasets: [{
      name: "Wind Speed", type: "line",
      values: [
        {{#each data.trendData.arrWind}}
          "{{this}}",
        {{/each}}
      ]
    }
    ]
  };
  new frappe.Chart("#chartwind", {
    data: data,
    type: "line",
    height: 180,
    colors: ["red"]
  });
</script>